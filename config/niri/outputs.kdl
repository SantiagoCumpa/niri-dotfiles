output "eDP-1" {
    // off
    mode "1920x1080@165"
    scale 1
    transform "normal"
    position x=0 y=0
}

output "DP-1" {
    // off
    mode "1920x1080@120.019"
    scale 1
    transform "normal"
    position x=1920 y=0
    focus-at-startup
}
